auto bridge
iface bridge
    bridge-vlan-aware yes
    bridge-vids 1
    bridge-ports {%- for interface in node["interfaces"].keys() -%}{%- if interface != "eth0" %} {{ interface }} {%- endif %} {%- endfor %}
  

auto vlan1
iface vlan1
   address {{ mgmt_ip }}
   vlan-raw-device bridge
 